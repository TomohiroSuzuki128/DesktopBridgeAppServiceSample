<Window x:Class="WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp"
        mc:Ignorable="d"
        Title="WPF アプリ MainWindow" Height="450" Width="800"
        Loaded="Window_Loaded">
	<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Height="450" Width="800" >
	<Button Content="UWPアプリの Page B へページ遷移を行い、データをUWP側に送る"
                Click="Button_Click" VerticalAlignment="Center" Height="50" FontSize="15" />
		<TextBlock x:Name="logTextBlock" VerticalAlignment="Center" />
		<TextBlock x:Name="uwpTextBlock" VerticalAlignment="Center" />
	</StackPanel>
</Window>
